{
	"manifest_version": 3,
	"name": "CcPiano",
	"version": "0.0.1",
	"description": "A simple piano popup extension for Chrome.",
	"permissions": [
		"contextMenus",
		"tabs",
		"storage"
	],
	"action": {
		"default_icon": "images/icon.png",
		"default_popup": "mainmenu.html",
		"default_title": "CcPiano"
	},
	"icons": {
	"16": "images/icon.png",
	"32": "images/icon.png",
	"48": "images/icon.png",
	"128":"images/icon.png"
	},
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["content.js"],
		"run_at": "document_end"
	}]
}